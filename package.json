{
  "name": "ms-docs",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docs:dev": "pnpm run register:components && vitepress dev docs",
    "docs:build": "pnpm run register:components && vitepress build docs --base /ms-docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.0.0",
    "swagger-ui-express": "^5.0.1",
    "vitepress": "^1.0.1",
    "vitepress-theme-demoblock": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "dependencies": {
    "ms-docs": "file:"
  }
}
